From 0000000000000000000000000000000000000000 Mon Sep 17 00:00:00 2001
From: Pekka Vuorela <pekka.vuorela@jolla.com>
Date: Fri, 10 Feb 2023 15:28:29 +0200
Subject: [PATCH] Map Xperia 10 III assistant button to camera

---
 keycodes/evdev         | 1 +
 symbols/jolla_vndr/sbj | 2 ++
 2 files changed, 3 insertions(+)

diff --git a/keycodes/evdev b/keycodes/evdev
index 02f834a1..ec635409 100644
--- a/keycodes/evdev
+++ b/keycodes/evdev
@@ -569,6 +569,7 @@ default xkb_keycodes "evdev" {
 	// End of autogenerated key codes
 
 	<I774> = 774;   // #define KEY_CAMERA_SNAPSHOT     766 - not in vanilla kernel
+	<I465> = 465;   // 457 - sony xperia 10 III assistant key
 
 	// Fake keycodes for virtual keys
 	<LVL3> =   92;
diff --git a/symbols/jolla_vndr/sbj b/symbols/jolla_vndr/sbj
index 8edcab91..c2b61e65 100644
--- a/symbols/jolla_vndr/sbj
+++ b/symbols/jolla_vndr/sbj
@@ -58,4 +58,6 @@ xkb_symbols "common-keys" {
     key <I536>  { [     XF86ContrastAdjust                                              ] };
     key <I774>  { [     XF86WebCam                                                      ] };
     key <I361>  { [     Select                                                          ] };
+    // Map Xperia 10 III assistant key to camera key
+    key <I465>  { [     XF86WebCam                                                      ] };
 };
